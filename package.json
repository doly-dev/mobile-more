{
  "name": "mobile-more",
  "description": "基于 antd-mobile v5 扩展移动端 UI 组件",
  "keywords": [
    "antd-mobile",
    "mobile-more",
    "mobile",
    "ui",
    "react"
  ],
  "version": "0.2.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs-version:build": "cross-env BUIDL_DOC_VERSION=version npm run docs:build",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "commit": "cz",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:stylelint -- --fix"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.less"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "npx --no -- commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "doly-icons": "^1.2.1",
    "rc-hooks": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/mockjs": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "add": "^2.0.6",
    "antd-mobile": "^5.0.0",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "father-build": "^1.17.2",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-two-style-order": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stylelint": "^14.3.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "util-helpers": "^4.2.1",
    "yarn": "^1.22.17",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd-mobile": "^5.0.0",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  }
}
