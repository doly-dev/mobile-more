{
  "name": "mobile-more",
  "description": "基于 antd-mobile v5 扩展移动端 UI 组件",
  "keywords": [
    "antd-mobile",
    "mobile-more",
    "mobile",
    "ui",
    "react"
  ],
  "version": "0.8.4",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs-version:build": "cross-env BUIDL_DOC_VERSION=version npm run docs:build",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build",
    "commit": "cz",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:stylelint -- --fix"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.less"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "npx --no -- commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "countdown-pro": "^1.0.2",
    "dayjs": "^1.11.5",
    "doly-icons": "^1.3.0",
    "rc-hooks": "^2.2.2",
    "rc-util": "^5.23.0",
    "util-helpers": "^4.12.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.7",
    "@types/mockjs": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@umijs/fabric": "^3.0.0",
    "@umijs/test": "^4.0.13",
    "add": "^2.0.6",
    "antd-mobile": "^5.22.0",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.47",
    "eslint": "^8.22.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "father-build": "^1.22.3",
    "lcn": "^4.1.1",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-two-style-order": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.11.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd-mobile": "^5.22.0",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "resolutions": {
    "@types/react": "18"
  }
}
