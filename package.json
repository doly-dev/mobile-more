{
  "name": "mobile-more",
  "description": "基于 antd-mobile v5 扩展移动端 UI 组件",
  "keywords": [
    "antd-mobile",
    "mobile-more",
    "mobile",
    "ui",
    "react"
  ],
  "version": "0.9.7",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs-version:build": "cross-env BUIDL_DOC_VERSION=version npm run docs:build",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "prepublishOnly": "npm run build",
    "commit": "cz",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint-fix:js": "npm run lint:js -- --fix",
    "lint:style": "stylelint src/**/*.less",
    "lint-fix:style": "npm run lint:stylelint -- --fix",
    "tsc": "tsc --noEmit",
    "prepare": "husky"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "lib"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.less"
  ],
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "author": "caijf <369756941@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "classnames": "^2.5.1",
    "countdown-pro": "^2.1.0",
    "dayjs": "^1.11.12",
    "doly-icons": "^1.5.4",
    "rc-hooks": "^3.0.12",
    "rc-util": "^5.43.0",
    "ut2": "^1.11.2",
    "util-helpers": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@umijs/fabric": "^3.0.0",
    "@umijs/types": "^3.5.42",
    "antd-mobile": "^5.37.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.54",
    "dumi-theme-default": "^1.1.24",
    "element-remove": "^1.0.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "father-build": "^1.22.5",
    "file-loader": "^6.2.0",
    "husky": "^9.1.4",
    "inquirer": "^9.3.6",
    "lcn": "^5.0.6",
    "lint-staged": "^15.2.7",
    "mockjs-extend": "^1.3.1",
    "prettier": "^3.3.3",
    "prettier-plugin-two-style-order": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "~4.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^6.1.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "antd-mobile": "^5.32.1",
    "react": ">=16.12.0",
    "react-dom": ">=16.12.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
